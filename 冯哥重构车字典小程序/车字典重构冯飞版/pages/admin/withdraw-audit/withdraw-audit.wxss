.container {
  color:#fff;
}

.withdraw_audit_box {
    padding: 0 30rpx 15px 30rpx;
}

.withdraw_audit_li {
    width: 100%;
    background: rgba(255, 255, 255, 0.2);
    margin-top: 15px;
    border-radius: 10rpx;
}

.withdraw_audit_li .info {
    padding: 0 30rpx;
    height: 187rpx;
    border-bottom: 1px solid #e6e6e6;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.withdraw_audit_li .info .intr {
    flex: 1;
}

.withdraw_audit_li .info .intr .p {
    font-size: 16px;
}

.withdraw_audit_li .info .intr .h1 {
    font-size: 30rpx;
    margin-top: 5px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between; 
}

.withdraw_audit_li .btn {
    border: 1px solid rgba(255, 255, 255, 0.3);
    border-radius: 5px;
    background: rgba(255, 255, 255, 0);
    width: 130rpx;
    height: 54rpx;
    line-height: 54rpx;
    text-align: center;
}

.withdraw_audit_li .btn.gary {
    border: 1px solid #999;
    background: rgba(255, 255, 255, 0.4);
}

.withdraw_audit_li .time {
    height: 68rpx;
    line-height: 68rpx;
    padding-left: 30rpx;
}
.price{
  width: 80%;
  font-size:50rpx;
  text-align: center;

}