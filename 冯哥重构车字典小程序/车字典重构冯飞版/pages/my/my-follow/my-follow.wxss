/* pages/my/my-fans/my-fans.wxss */
.fans{
  padding: 10rpx 20rpx;
  color: #fff;
  letter-spacing: 2rpx;
}
.fans_list{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between; 
  padding: 15rpx 0rpx;
  border-bottom: 1rpx solid rgba(255, 255, 255, 0.2)
}
.left{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between; 
}
.xinxi text{
  margin-right: 10rpx;
}
.left image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 100rpx;
}

.right image{
  width: 120rpx;
  height: 60rpx;
}
